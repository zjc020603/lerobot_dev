{
  "dataset_mixture": {
    "datasets": [
      {
        "repo_id": "Jiacheng0603/handover_lerobot_dataset_208trajs_pi05_task_cmd_crop"
      }
    ],
    "weights": [
      1.0
    ],
    "action_freq": 30.0,
    "image_resample_strategy": "nearest",
     "vector_resample_strategy": "nearest"
  },
  "policy": {
     "type": "pi05",
     "push_to_hub": false,
     "pretrained_path": "lerobot/pi05_base",
     "advantage": "on",
     "n_obs_steps": 1,
     "chunk_size": 50,
     "n_action_steps": 50,   
     "dtype": "bfloat16",
     "device": "cuda",
     "max_state_dim": 32,
     "max_action_dim": 32,
     "state_cond": true,
     "normalization_mapping": {
         "VISUAL": "IDENTITY",
         "STATE": "MEAN_STD",
         "ACTION": "MEAN_STD"
     },
     "freeze_vision_encoder": false,
      "train_expert_only": false,
     "tokenizer_max_length": 200
    },

  "steps": 60000,
  "batch_size": 8, 
  "num_workers": 4,
  "use_policy_training_preset": false,
  "optimizer": {
    "type": "adamw",
    "lr": 5.0e-5,
    "betas": [0.9, 0.95],
    "weight_decay": 1.0e-10
  },
  "scheduler": {
    "type": "cosine_decay_with_warmup",
    "num_warmup_steps": 1000,
    "peak_lr": 5.0e-5,
    "decay_lr": 2.5e-6,
    "num_decay_steps": 50000
  },
  "save_freq": 5000,
  "eval_freq": 0,
  "wandb": {
    "enable": true,
    "project": "vlash",
    "entity": "zhang20020603-lund-university",
    "disable_artifact": true
  }
}
